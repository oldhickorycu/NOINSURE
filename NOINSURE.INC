[Associate Database to Formdata]
PROCEDURE ASSIGNFIELDVARIABLES
[ Variable for Field #     Length]
  TOTALINDEX = 0
  FIELDMAX =   12
  DATACOUNT( 1) = 0012

  TEMPCHAR = ANUMBER
  CALL REMOVELEADINGZEROS

  FORMDATA(001)=""
  FORMDATA(002)=TEMPCHAR+"-"+LAID
  FORMDATA(003)=FORMAT("99/99/99",SYSTEMDATE)
  FORMDATA(004)=FORMAT("99/99/99",SYSTEMDATE)
  FORMDATA(005)=""
  FORMDATA(006)=FORMAT("99/99/99",SYSTEMDATE)
  FORMDATA(007)=""
  FORMDATA(008)=FORMAT("99/99/99",SYSTEMDATE)
  FORMDATA(009)=""
  FORMDATA(010)=FORMAT("99/99/99",SYSTEMDATE)
  FORMDATA(011)=SYSUSERNAME(SYSUSERNUMBER)
  FOR ACCOUNT ACCOUNT:NUMBER
   DO    
    FOR EACH LOAN WITH (LOAN:ID = LID) 
     DO 

      FORMDATA(012)=FORMAT("9999",VALUE(LOAN:APPLICATIONID))  
     END
   END
  FOR FORMLOOP = 1 TO FIELDMAX
  DO
   TEMPCHAR = FORMDATA(FORMLOOP)
   CALL SLASHOUTPUT
   FORMDATA(FORMLOOP)=TEMPCHAR
  END
END

[Associate FORMDATA back to RD field]
PROCEDURE UPDATEFIELDVARIABLES
END
